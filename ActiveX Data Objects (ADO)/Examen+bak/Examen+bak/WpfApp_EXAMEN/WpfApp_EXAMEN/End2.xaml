<Window x:Class="WpfApp_EXAMEN.End2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp_EXAMEN"
        mc:Ignorable="d"
        Title="End2" Height="450" Width="800" Background="Black">
    <Grid >
        <StackPanel  Margin="20" >
            <Label Content="This is End!" Foreground="White" FontSize="27" HorizontalAlignment="Center"/>
            <Separator></Separator>
            <Label Name="lblnam" Content="User:" Foreground="White" FontSize="24"/>
            <Label Name="lbltime" Content="Time:" Foreground="White" FontSize="24"/>
            <Label  Content="Congratulation! " Foreground="White" FontSize="24" HorizontalAlignment="Center"/>
            <Label  Content=" You do not have this serfiticate,because your score is low than 10! " Foreground="White" FontSize="24" HorizontalAlignment="Center"/>
            <Label  Content="Try again to have serfiticate! " Foreground="White" FontSize="24" HorizontalAlignment="Center"/>
            <Label  Content=" Thank you  for the test :)" Foreground="White" FontSize="24" HorizontalAlignment="Center"/>
            <Button Name="btnSubmit" Content="End" Margin="60 10" Width="200"
                    Background="DarkCyan" Foreground="White" FontSize="24" Click="btnEnd_Click"/>
        </StackPanel>


    </Grid>
</Window>
