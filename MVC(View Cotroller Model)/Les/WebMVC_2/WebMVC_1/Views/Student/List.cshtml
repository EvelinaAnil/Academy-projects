@using WebMVC_1.Models
@model IEnumerable<Student>

@*@model IEnumerable<WebMVC_1.Models.Student>*@

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>
    <div>
    </div>
    <br />

    Count: @Model.Count()
    <br />

    <div>
        @foreach (Student item in Model)
        {
            <h2>
                @item.LastName
            </h2>
            <h3>@item.FirstName</h3>
            <p>@item.ToString()</p>
        }
    </div>

    <br />
    <div>
        
    </div>
</body>
</html>
